@model Gweone.Models.ECSService
<div class="box" id="pagedList">
    <div class="box-header with-border">
        <h3 class="box-title">Process Command</h3>
        <div class="box-tools">

        </div>
    </div>
    @using (Html.BeginForm("Interactive", "TaskManager", FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
    {
        <div class="box-body">
            @Html.AntiForgeryToken()
            @Html.ValidationSummary("", new { @class = "text-danger" })
            <div class="form-group">
                @Html.LabelFor(m => m.ServicePath, new { @class = "col-md-2 control-label" })
                <div class="col-md-10">
                    @Html.TextBoxFor(m => m.ServicePath, new { @class = "form-control" })
                </div>
            </div>
            <div class="form-group">
                @Html.LabelFor(m => m.ServiceCommand, new { @class = "col-md-2 control-label" })
                <div class="col-md-10">
                    @Html.TextAreaFor(m => m.ServiceCommand, new { @class = "form-control" })
                </div>
            </div>
        </div>
        <div class="box-footer">
            <button type="submit" class="btn btn-primary pull-right">Execute</button>
        </div>
    }
</div>